<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yqhp.console.repository.mapper.PlanMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yqhp.console.repository.entity.Plan">
        <id column="id" property="id" />
        <result column="project_id" property="projectId" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="devices" property="devices" typeHandler="com.yqhp.console.repository.typehandler.PlanDevicesTypeHandler" />
        <result column="actions" property="actions" typeHandler="com.yqhp.console.repository.typehandler.PlanActionsTypeHandler" />
        <result column="run_mode" property="runMode" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="create_by" property="createBy" />
        <result column="update_by" property="updateBy" />
    </resultMap>

</mapper>
